<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.unocode.runnershigh_enhanced"
      android:versionCode="452"
      android:versionName="4.52"
      android:installLocation="preferExternal">
      <supports-screens
        android:smallScreens="true"
        android:normalScreens="true"
        android:largeScreens="true"
        android:anyDensity="true"
        />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <!-- added the android:name="com.orm.SugarApp part for SugarORM -->
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:name="com.orm.SugarApp">
        <!-- google play games services -->
        <meta-data android:name="com.google.android.gms.games.APP_ID"
            android:value="@string/app_id" />
        <meta-data android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>
        <!--         **           -->

        <!-- sugar orm -->
        <meta-data android:name="DATABASE" android:value="sugar_example.db" />
        <meta-data android:name="VERSION" android:value="2" />
        <meta-data android:name="QUERY_LOG" android:value="true" />
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="com.unocode.runnershigh_enhanced" />
        <!--         **           -->

        <activity android:name=".main"
                  android:label="@string/app_name"
                  android:screenOrientation="landscape"
				  android:theme="@android:style/Theme.NoTitleBar.Fullscreen">        
        </activity>
		
		<activity android:name=".HighScoreActivity" 
				  android:screenOrientation="landscape"
				  android:theme="@android:style/Theme.NoTitleBar.Fullscreen"/>
		<activity android:name=".Menu"
				  android:screenOrientation="landscape"
				  android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
		    <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HighScoreForm"
        		  android:screenOrientation="landscape"
				  android:theme="@android:style/Theme.NoTitleBar.Fullscreen"/>
		<activity android:name=".Info" 
				  android:screenOrientation="landscape"
				  android:theme="@android:style/Theme.NoTitleBar.Fullscreen" />
    </application>
</manifest> 